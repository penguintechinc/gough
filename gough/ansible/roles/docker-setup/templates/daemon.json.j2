{
  "log-driver": "{{ docker_config.log_driver }}",
  "log-opts": {{ docker_config.log_opts | to_json }},
  "storage-driver": "{{ docker_config.storage_driver }}",
  "data-root": "{{ docker_config.data_root }}",
{% if docker_config.insecure_registries | length > 0 %}
  "insecure-registries": {{ docker_config.insecure_registries | to_json }},
{% endif %}
{% if docker_config.registry_mirrors | length > 0 %}
  "registry-mirrors": {{ docker_config.registry_mirrors | to_json }},
{% endif %}
  "metrics-addr": "{{ docker_config.metrics_addr }}",
  "experimental": {{ docker_config.experimental | lower }},
  "features": {{ docker_config.features | to_json }},
  "default-ulimits": {{ docker_config.default_ulimits | to_json }},
  "live-restore": true,
  "userland-proxy": false,
  "ip-forward": true,
  "ip-masq": true,
  "iptables": true,
  "ipv6": false,
  "fixed-cidr": "172.17.0.0/16",
  "bridge": "docker0"
}